{
  "extends": [
    "plugin:nuxt/recommended",
    "standard"
    // "plugin:vue/vue3-recommended"
  ],
  "rules": {
    "vue/multi-word-component-names": "off",
    "space-before-function-paren": "off"
  },
  "globals": {
    "defineNuxtConfig": true,
    "useRoute": true,
    "useRouter": true,
    "onMounted": true,
    "ref": true,
    "computed": true,
    "useRuntimeConfig": true,
    "watch": true
  }
}